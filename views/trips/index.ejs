<%- include('../partials/header') %>

<table id="trip-list">
  <thead>
    <tr>
      <th>Trip Name</th>
      <th>Location</th>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Budget</th>
      <th>Expenses</th>
      <th>Privacy</th>
    </tr>
  </thead>
  <tbody>
    <% trips.forEach(function(t) { %>
      <tr>
        <td><%= t.title %></td>
        <td><%= t.location %></td>
        <td><%= t.startDate %></td>
        <td><%= t.endDate %></td>
        <td><%= t.budget %></td>
        <td><%= t.expenses %></td>
        <td><%= t.privacy %></td>
        <td><a href="/trips/<%= t._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>